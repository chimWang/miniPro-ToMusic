<view class='container'>
  <view catchtap='back' class='back'>
   <image src='/images/playMusic/back.svg'></image>
  </view>
  <view class='background'>
    <image src='/images/playMusic/background.jpg' mode='aspectFill'></image>
  </view>
  <view class='title'>
    <image src='/images/playMusic/music.svg'></image>
    <text>Jungle Pink</text>
  </view>
  <view class='player'>
    <view>
      <image src='/images/playMusic/last.svg'></image>
    </view>
    <view>
      <audio id='myAudio' src='http://47.100.112.48/music/Aimer.mp3' bindtimeupdate='musicStart'></audio>
      <image src='{{isPlayingMusic?"/images/playMusic/pause.svg":"/images/playMusic/start.svg"}}' catchtap='onMusicTap'></image>
    </view>
    <view>
      <image src='/images/playMusic/next.svg'></image>
    </view>
  </view>
  <view class='musicProgress'>
    <view class='musicBg'></view>
    <view style='width:{{progress}}%;'>
      <view class='musicBtn'></view>
    </view>
  </view>
</view>